{
    "name": "bsolut/magento_module_base,
    "type": "magento-module",
    "license": "GPL-2.0+",
    "description":"Magento extension STUB",
    "keywords": ["sample", "module", "unittest"],
    "require": {
        "magento-hackathon/magento-composer-installer": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "n98/magerun":"*",
        "colinmollenhour/modman":"dev-master",
        "magetest/magento-phpunit-extension": "dev-develop"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.firegento.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/BSolut/modman.git"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist",
        "github-protocols:": ["https", "ssh", "git"],
        "cache-files-maxsize": "2048MiB"
    },
    "extra": {
        "magento-root-dir": "./tmp"
    },
    "minimum-stability": "dev",
    "authors":[
        {
            "name":"BSolut GmbH",
            "email":"contact@bsolut.com",
            "homepage":"https://bsolut.com"
        }
    ]
}
